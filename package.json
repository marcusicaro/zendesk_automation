{
  "name": "zendesk-ticket-automation",
  "version": "1.0.0",
  "description": "Intelligent Zendesk ticket triage automation system",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "npm run build && node dist/index.js",
    "start:ts": "node --loader ts-node/esm src/index.ts",
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "npm run build && node dist/index.js --test",
    "test:ts": "node --loader ts-node/esm src/index.ts --test",
    "demo": "node create-demo-tickets.js",
    "lint": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "zendesk",
    "automation",
    "ticket-triage",
    "customer-support"
  ],
  "author": "Marcus",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.12.2",
    "dotenv": "^17.2.2",
    "moment": "^2.30.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
